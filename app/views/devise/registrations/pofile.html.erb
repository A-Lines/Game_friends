<h2>Sign up2</h2>

<%= form_with model: @user, url: registrations_path(@user), local: true do |f| %>

  <div class="field">
    <%= f.label :nickname %><br />
    <%= f.email_field :email, autofocus: true, autocomplete: "email" %>
  </div>

    <% f.hidden_field :email %>
    <% f.hidden_field :password %>

  <div class="actions">
    <%= f.submit "Sign up" %>
  </div>
<% end %>


    



